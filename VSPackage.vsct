    <KeyBinding guid="guidVsToolsCmdSet" id="PasteAsComments" editor="guidVSStd97" mod1="ALT" key1="L" mod2="ALT" key2="V"  />
    <KeyBinding guid="guidVsToolsCmdSet" id="LineupDeclarations" editor="guidVSStd97" mod1="ALT" key1="L" mod2="ALT" key2="L"  />
