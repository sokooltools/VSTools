      <!-- =============================================================================================================
			 To define a group we have to specify its ID, the parent and its display priority.
			 In this case we use as our parent menu the Tools menu of VS; its ID is defined in vsshlids.h.
			 ============================================================================================================= -->
      <Group guid="guidVsToolsCmdSet" id="MyGroupInToolsMenu" priority="0x600">
        <Parent guid="guidSHLMainMenu" id="IDM_VS_MENU_TOOLS"/>
      </Group>

      <!-- =============================================================================================================
			 The second group is created inside the main toolbar of Visual Studio.
			 Again, you can find the ID inside vsshlids.h.
			 ============================================================================================================= -->
